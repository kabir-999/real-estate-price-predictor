# Ignore Python cache files
__pycache__/
*.py[cod]

# Ignore virtual environment folders
venv/
env/

# Ignore model files
models/*.pkl
models/*.h5
models/*.joblib

# Ignore dataset files
data/*.csv
data/*.xlsx

# Ignore log files
*.log

# Ignore system files
.DS_Store  # macOS
Thumbs.db  # Windows

# Ignore Jupyter Notebook checkpoints
.ipynb_checkpoints/

# Ignore environment files
.env
